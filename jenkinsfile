pipeline {
    agent any
    stages {
        stage("Build") {
            steps {
                echo "Building..."
            }
            post {
                echo "Post-build!"
            }
        }
        stage("Test") {
            steps {
                echo "Testing..."
            }
        }
        stage("Deploy") {
            steps {
                echo "Deploying..."
            }
        }
        stage("Complete") {
            steps {
                echo "Completed!~!1"
            }
        }
    }
}